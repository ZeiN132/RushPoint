local knife = game:GetService("ReplicatedStorage").Assets.Weapons.Knife:FindFirstChild("Sweet Delight"):Clone()
knife.Parent = game:GetService("ReplicatedStorage").Assets.Weapons.Knife
knife.Name = "Sweet Delight"
wait(0.5)
game:GetService("ReplicatedStorage").Assets.Weapons.Knife.Default:Destroy()
wait(0.5)
game:GetService("ReplicatedStorage").Assets.Weapons.Knife:FindFirstChild("Sweet Delight").Name = "Default"
wait(0.5)
